<p style="color: green"><%= notice %></p>

<% content_for :title, "Downloads" %>

<h1>Downloads</h1>

<div id="downloads">
  <% @downloads.each do |download| %>
    <%= render download %>
    <p>
      <%= link_to "Show this download", download %>
    </p>
  <% end %>
</div>

<%= link_to "New download", new_download_path %>
